function resizeIframe(obj) {
  obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
  $("#popupVisitenkarteRahmen").css("height", obj.contentWindow.document.body.scrollHeight + 'px');
}

jQuery(document).ready(function ($) {

  // ToDo: Muss stattdessen direkt im Template entfernt werden
  $('#subheader').remove();

  /*********************************************
   *  Doppelte (gleichlautende) Überschriften entfernen
   */
  var headline = $('#headline h1').text().trim();

  //var header = $('#main > *:first-child > header > *');
  // Gleichlautende Überschriften aus dem Inhalt entfernen
  var header = $('#main h1, #main h2, #main h3, #left h1, #left h2, #left h3');
  $(header).each(function () {
    if ($(this).text().trim() == headline)
      $(this).hide();
  });
//    var header = $('#main header > *');
//    var header0 = header[0];
  var header0 = $('#main header > *').first();
  var headertext = $(header0).text().trim();
  if (headline == headertext)
    $(header0).parent().css('display', 'none');
  $('#instbar a').each(function () {
    if (($(this).parent().hasClass('abt') /*|| $(this).parent().hasClass('einr') || $(this).parent().hasClass('fk')*/) && $(this).text().trim() == headline) {
      $(this).parent().remove(); // Aus den blauen Balken entfernen
      //$('#headline h1').text('') // Seitentitel entfernen
    } else {
      if ($(this).text() == headline)
      //$(this).parent().remove(); // Aus blauem Balken entfernen
        $('#headline h1').text(''); // Seitentitel entfernen
    }
  });

  if(!($('#left *').length) && !($('#wrapper.one-column').length)) {
    $('#wrapper-content').addClass('noleft');
  }

  /*************************************************
   *  Navizeile fix positionieren
   */
  var THRESHOLD = 270; //282;
  /*if($('#navizeile_container').length) {
    THRESHOLD = 143; // bei 1-Spalten-Layout
  }
  */
  var OLDSCROLL = 0;
  var mitlogo = $('#content_header img').length; // false
  var suche_click_trigger = false;
  var $nav = $('#header');
  var $header = $('#undefined-sticky-wrapper .header-sticky').first();
  var lastScrollTop = 0;
  var maxScrollTop = 0;
  var direction;

  $(window).scroll(function () {

    if ($(window).width() > 1100) {
      if ($(this).scrollTop() > THRESHOLD && OLDSCROLL <= THRESHOLD ) {
        $('body').addClass('scrolled');
        setNavizeileGescrollt(mitlogo);
        setSucheMenue();
        OLDSCROLL = $(this).scrollTop();
      }
      if (($(this).scrollTop() < THRESHOLD) && OLDSCROLL >= THRESHOLD ) {
        $('body').removeClass('scrolled');
        setSucheKopf();
        setNavizeileFix(mitlogo);
        OLDSCROLL = $(this).scrollTop();
      }
      return true;
    }

    else {

      var scrollTop = $(this).scrollTop();
      $('#undefined-sticky-wrapper .header-sticky').first().toggleClass('schatten',scrollTop>15);
      if (lastScrollTop < scrollTop && scrollTop > $('#undefined-sticky-wrapper .header-sticky').first().outerHeight() && direction != 'down') {
        //Scroll down
        if($('ul.slicknav_nav').first().css('display')=='none') {
          $nav.stop().fadeOut();
          direction = 'down';
        }
      } else if (lastScrollTop > scrollTop  && direction != 'up') {
        // Scroll up
        $nav.stop().fadeIn(300);
        direction = 'up';
      }
      lastScrollTop = scrollTop;
    }
});

  //function setNavizeileFix(mitlogo = false) { /* js-Fehler in IE 2018-08-07#LV */
  function setNavizeileFix(mitlogo) {
    if($('#navizeile_container').length) {
      $('#navizeile').removeClass('sticky').unwrap('#nzcont').appendTo('#navizeile_container');
    } else {
      if (mitlogo) {
