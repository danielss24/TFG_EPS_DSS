      if (parseInt($instbar_inner.css('font-size')) >= 16) {
        $instbar_inner.css('font-size','');
      }
    }
  }

  /* Kiwi-Accordion */
  function kiwi(kiwineu) {

  // Falls die Konstante nicht gesetzt wurde...
    if (typeof tx_kiwiaccordion_effect == 'undefined') {
      tx_kiwiaccordion_effect = 'none';
    }
    $('.ka-panel'+kiwineu).each(function () {
      //Erste Überschrift suchen
      $header = $(':header:first', this);
      //Fehler Behandlung wenn keine Überschrift vorhanden ist
      if ($header.length == 0) {
        $(this).addClass('ka-error').removeClass('ka-panel');
        console.error('This panel contains no header.', this);
      } else {
        //kleiner trick um <div class="csc-header"><h1>... abzufangen
        if ((($header.parent().find('*').length == 1) || $header.parent().is("header") || $header.parent().hasClass("csc-header")) && !$header.parent().is('.ka-panel')) {
          $header.parent().addClass('ka-handler');
        } else {
          $header.addClass('ka-handler');
        }

        //Inhalte umschließen für die Ansprache
        if ($header.closest('.ka-handler').next().hasClass('ka-content')) {
        } else {
          $header.closest('.ka-handler').siblings().wrapAll('<div class="ka-content"></div>');
        }
      }
    });
// Versteckte Inhalte nicht anzeigen
    $('.ka-panel'+kiwineu+'.closed .ka-content').hide();
// Für ein paar Effekte
    $('.ka-panel'+kiwineu+' .ka-handler').hover(function () {
      $(this)/*.parents*/.closest('.ka-panel').addClass('hover');
    }, function () {
      $(this)/*.parents*/.closest('.hover').removeClass('hover');
    });
// Eventhandler
    //$(document).on( 'click', '.ka-panel'+kiwineu+' .ka-handler', function (event, data) {
    $('.ka-panel'+kiwineu+' .ka-handler').off('click').click(function (event, data) {
      $panel = $(this)/*.parents*/.closest('.ka-panel');
      $content = $panel.find('.ka-content');
      if ($panel.is('.closed')) {
        $('.ka-panel.ka-opend').removeClass('ka-opend');
        $content.first().slideDown();
        $panel.removeClass('closed').removeClass('close').addClass('open');
        //Wenn nur ein offenes Panel erlaubt ist, andere Panels schließen
        if (tx_kiwiaccordion_exclusive) {
          $('.ka-panel.open .ka-handler').trigger('click', {clicked: $('.ka-panel').index($panel)});
        }
      } else {
        if (!data) {
          data = {clicked: -1};
        }
        if (data.clicked != $('.ka-panel').index($panel)) {
          $content.slideUp(100);
          $panel.removeClass('open').addClass('closed');
          $panel.find('.ka-panel').removeClass('open').addClass('closed');
        }
      }
    });
  }

